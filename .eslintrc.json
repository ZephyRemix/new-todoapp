{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-plusplus": "off",
    "import/no-cycle": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "import/no-named-as-default": "off",
    "import/no-named-as-default-member": "off",
    "no-underscore-dangle": "off"
  }
}
